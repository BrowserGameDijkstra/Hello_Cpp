#include <iostream> 
using namespace std;
void ZeichneZeichen(int, char); 
int main()
{ for (int i = 0; i < 26; i++) 
ZeichneZeichen(i + 1, 'A' + i); 
return 0; }
void ZeichneZeichen(int n, char z) {
for (int i = 0; i < n; i++) cout << z;
cout << endl;
}
