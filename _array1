#include <iostream>
using namespace std;

void printArray(int arr[], int size) {
    for(int x=0; x<size; x++) {
        cout <<arr[x]<< endl;
    }
}

int main() {
    int myArr[3]= {42, 33, 88};
    printArray(myArr, sizeof(myArr)/4);
}
#include <iostream>
using namespace std;

void printArray(int arr[], int size) {
    for(int x=0; x<size; x++) {
        cout <<arr[x]<< endl;
    }
}

//You don't even have to exactly know the size of myArr 
